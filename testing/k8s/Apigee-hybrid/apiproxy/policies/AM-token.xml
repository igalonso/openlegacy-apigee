<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-token">
    <DisplayName>AM-token</DisplayName>
    <Properties/>
    <Add>
        <Headers>
            <Header name="Authorization">Bearer {idTokenPrefix.access_token}</Header>
        </Headers>
    </Add>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>